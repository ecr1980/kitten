<%= form_with model: @kitty do |f| %>
  <%= f.label "Kitty's name:" %><br>
  <%= f.text_field :name %><br>
  <%= f.label "Age:" %><br>
  <%= f.number_field :age, in: 0..100 %><br>
  <%= f.label "Cuteness:" %><br>
  <%= f.range_field :cuteness, in: 1..10 %><br>
  <%= f.label "Softness:" %><br>
  <%= f.range_field :softness, in: 1..10 %><br>
  <%= f.submit "Save!" %>
<% end %>